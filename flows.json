    {
        "id": "20635778ecbf6fe0",
        "type": "function",
        "z": "2bf117dae05b2643",
        "name": "function 21",
        "func": "let m1 = context.get(\"msg1\");\nlet m2 = context.get(\"msg2\");\nlet btn = context.get(\"button\");\n\n// Store depending on topic\nif (msg.topic === \"msg1\") {\n    m1 = msg.payload;\n    context.set(\"msg1\", m1);\n}\nelse if (msg.topic === \"msg2\") {\n    m2 = msg.payload;\n    context.set(\"msg2\", m2);\n}\nelse if (msg.topic === \"button\") {\n    btn = msg.payload;\n    context.set(\"button\", btn);\n}\n\n// Output only when both are available\nif ((m1 !== null && m1 !== undefined) && \n    (m2 !== null && m2 !== undefined) && \n    (btn !== null && btn !== undefined)) {\n    // Optional: reset after sending\n    // context.set(\"msg1\", null);\n    // context.set(\"msg2\", null);\n    context.set(\"button\", null);\n\n    msg.topic = \"message\";\n    return {\n        topic: \"message\",\n        payload: {\n            msg1: m1,\n            msg2: m2\n        }\n    };\n}\n\nreturn null;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
